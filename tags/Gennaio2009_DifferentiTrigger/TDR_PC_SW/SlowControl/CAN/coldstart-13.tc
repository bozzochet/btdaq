// file coldstart-13.tc
//
// Alexei Lebedev & Claude Zurbach May-2008...
//
// Cold start procedure : no aids, free-running PPS, inhibition all TM excepted STIME (10s)
//
// WMODE : from init mode to navigation mode (default : 1234)
WMODE:
CONFIG_FOR_PPS    0

delay 9

// SPVDAT : inhibit with period 0 sec
GMESS:
TM_ID             12
PERIOD            0

delay 1

// RSYSTEM-COA : inhibit
RSYSTEM-COA:

delay 1

// SNAVI :  inhibit with period 0 sec
GMESS:
TM_ID           4
PERIOD          0

delay 1

// SNAVN :  inhibit with period 0 sec
GMESS:
TM_ID           5
PERIOD          0

delay 1

// SNAVS :  inhibit with period 0 sec
GMESS:
TM_ID           6
PERIOD          0

delay 1

// SRECEIVST :  inhibit with period 0 sec
GMESS:
TM_ID           13
PERIOD          0

delay 1

// SCOSDIR :  inhibit with period 0 sec
GMESS:
TM_ID           11
PERIOD          0

delay 1

// S1RAWD :  inhibit with period 0 sec (valid for S1RAWD, S2RAWD and S3RAWD)
GMESS:
TM_ID           14
PERIOD          0

delay 1

// S1CHANST : inhibit with period 0 sec
GMESS:
TM_ID           17
PERIOD          0

delay 1

// NRES : inhibit with period 0 sec
GMESS:
TM_ID           25
PERIOD          0

delay 1

// NSXVECT : inhibit with period 0 sec
GMESS:
TM_ID           20
PERIOD          0

delay 1

// STIME : with period 10 sec
GMESS:
TM_ID          3
PERIOD         10

delay 1
