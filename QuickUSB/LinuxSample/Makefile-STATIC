####### Build rules

all: Makefile-STATIC $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile-STATIC: QuickUsbDiagStatic.pro  /usr/lib/qt-3.3/mkspecs/default/qmake.conf  \
		/usr/lib/qt-3.3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile-STATIC QuickUsbDiagStatic.pro
qmake: 
	@$(QMAKE) -o Makefile-STATIC QuickUsbDiagStatic.pro

dist: 
	@mkdir -p .obj/QuickUsbDiagStatic && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/QuickUsbDiagStatic/ && $(COPY_FILE) --parents QuickUSB_Logo.jpeg .obj/QuickUsbDiagStatic/ && $(COPY_FILE) --parents QuickUsbDiag.ui.h outputForm.ui.h AboutForm.ui.h .obj/QuickUsbDiagStatic/ && ( cd `dirname .obj/QuickUsbDiagStatic` && $(TAR) QuickUsbDiagStatic.tar QuickUsbDiagStatic && $(GZIP) QuickUsbDiagStatic.tar ) && $(MOVE) `dirname .obj/QuickUsbDiagStatic`/QuickUsbDiagStatic.tar.gz . && $(DEL_FILE) -r .obj/QuickUsbDiagStatic

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
